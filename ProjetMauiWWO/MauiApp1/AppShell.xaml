<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiApp1.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiApp1"
    xmlns:view="clr-namespace:MauiApp1.View"
    Title="MauiApp1"
    Shell.FlyoutBehavior="Flyout">



    <ShellContent
        Title="Menu"
        ContentTemplate="{DataTemplate view:Menu}"
        Route="Menu" />
    <ShellContent
        Title="Connexion"
        ContentTemplate="{DataTemplate view:ConnexionView}"
        Route="ConnexionView" />
    <ShellContent
        Title="Stagiaire"
        ContentTemplate="{DataTemplate view:Stagiaire_Page}"
        Route="Stagiaire_Page" />
    <ShellContent
        Title="Stage"
        ContentTemplate="{DataTemplate view:Stage_Page}"
        Route="Stage_Page" />

    <ShellContent
        Title="Entreprise"
        ContentTemplate="{DataTemplate view:PageEntrepriseWWO}"
        Route="PageEntrepriseWWO" />


    <ShellContent
        Title="Stage"
        ContentTemplate="{DataTemplate view:CandidatureDraftView}"
        Route="CandidatureDraftView" />


    <ShellContent
        Title="Stage"
        ContentTemplate="{DataTemplate view:DemandeCandidatView}"
        Route="DemandeCandidatView" />






</Shell>


